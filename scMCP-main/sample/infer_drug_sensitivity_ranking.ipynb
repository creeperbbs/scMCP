    "import numpy as np, pandas as pd, matplotlib.pyplot as plt\n",
    "from scipy.stats import weightedtau, gaussian_kde\n",
    "from matplotlib.colors import LinearSegmentedColormap\n",
    "\n",
    "plt.rcParams['font.family'] = 'sans-serif'\n",
    "plt.rcParams['font.sans-serif'] = ['Helvetica']\n",
    "plt.rcParams['savefig.dpi'] = 600\n",
    "plt.rcParams['figure.subplot.wspace'] = 0.25\n",
    "\n",
    "palette = {\n",
    "    'distance': '#FF6F61',  \n",
    "    'scMCP': '#4E79A7',    \n",
    "    'PRNet': '#59A14F',    \n",
    "    'Random': '#BAB0AC'}    \n",
    "\n",
    "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(12, 4.5))\n",
    "\n",
    "k_list = np.array([40,60,80,100])\n",
    "hit_rates = np.array([len(set(exp_df.head(k)['Drug Name']) & set(model_df.head(k)['drug'])) / k for k in k_list])\n",
    "\n",
    "x_pos = np.array([2,4,6,8] )   \n",
    "width = 0.05                   \n",
    "compact_x = x_pos * width         \n",
    "\n",
    "bar_colors = ['#FF6F61', '#4E79A7', '#59A14F', '#F28E2C']\n",
    "for i, (xi, hi) in enumerate(zip(compact_x, hit_rates)):\n",
    "    ax1.bar(xi, hi, width, color=bar_colors[i], alpha=0.95,\n",
    "            edgecolor='black', linewidth=0.6)\n",
    "ax1.scatter(compact_x, hit_rates, color='black', s=36, zorder=3)\n",
    "ax1.set_xticks(compact_x)\n",
    "ax1.set_xticklabels(k_list)\n",
    "ax1.set_xlabel('Top-k', fontsize=11)\n",
    "ax1.set_ylabel('Hit rate', fontsize=11)\n",
    "ax1.set_ylim(0, 1.05)\n",
    "sns.despine(ax=ax1, offset=4, trim=True)\n",
    "ax1.text(0.03, 0.97, f'SHP-77\\n(k={k_list[-1]})', transform=ax1.transAxes,\n",
    "         fontsize=10, bbox=dict(boxstyle='round', facecolor='wheat', alpha=0.8))\n",
    "from scipy.stats import spearmanr, weightedtau, pearsonr  \n",
    "\n",
    "top20_exp = exp_df.head(80).reset_index(drop=True)\n",
    "top20_mdl = model_df.head(80).reset_index(drop=True)\n",
    "top20_exp['rank_exp'] = range(1,81)\n",
    "top20_mdl['rank_mdl'] = range(1,81)\n",
    "merge_df = pd.merge(top20_exp[['Drug Name', 'rank_exp']],\n",
    "                    top20_mdl[['drug', 'rank_mdl']],\n",
    "                    left_on='Drug Name', right_on='drug', how='inner')\n",
    "\n",
    "x = merge_df['rank_exp']\n",
    "y = merge_df['rank_mdl']\n",
    "\n",
    "# 1. tau\n",
    "rho_w, p_w = weightedtau(x, y)\n",
    "\n",
    "# 2. Spearman   p-value\n",
    "rho_s, p_s = spearmanr(x, y)\n",
    "\n",
    "# 3. Pearson R\n",
    "r_pearson, p_p = pearsonr(x, y)\n",
    "r2 = r_pearson \n",
    "\n",
    "# 4. Dense\n",
    "xy = np.vstack([x, y])\n",
    "z = gaussian_kde(xy)(xy)\n",
    "\n",
    "# 5. plot\n",
    "cmap = LinearSegmentedColormap.from_list('warm', ['#FFEB3B', '#FF9800', '#F44336'], N=256)\n",
    "points = ax2.scatter(x, y, c=z, s=80, cmap=cmap,\n",
    "                     edgecolors='black', linewidths=0.4, zorder=3)\n",
    "ax2.plot([1,100], [1,100], '--', color='#7F7F7F', linewidth=1.2, alpha=0.8)\n",
    "ax2.fill_between(np.arange(1, 81), np.arange(1, 81) - 3, np.arange(1,81) + 3,\n",
    "                 color='grey', alpha=0.15, zorder=0)\n",
    "\n",
    "ax2.set_xlabel('Experiment rank', fontsize=11)\n",
    "ax2.set_ylabel('Model rank', fontsize=11)\n",
    "ax2.set_aspect('equal')\n",
    "ax2.set_xlim(0.8, 80.2)\n",
    "ax2.set_ylim(0.8, 80.2)\n",
    "sns.despine(ax=ax2, offset=4, trim=True)\n",
    "\n",
    "ax2.text(0.03, 0.97,\n",
    "         f'Weighted  = {rho_w:.3f}\\n'\n",
    "         f'Spearman  = {rho_s:.3f} (p = {p_s:.2e})\\n'\n",
    "         f'Pearsonr = {r2:.3f}',\n",
    "         transform=ax2.transAxes, fontsize=10, va='top',\n",
    "         bbox=dict(boxstyle='round', facecolor='wheat', alpha=0.8))\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
